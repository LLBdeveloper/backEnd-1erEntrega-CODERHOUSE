<div >
    <h1>Productos agregados hasta el momento</h1>
    <div id="allProducts">
        {{#each products}}
            <div class="products">
                <h2>{{this.title}}</h2>
                <p><strong>Descripción:</strong> {{this.description}}</p>
                <p><strong>Precio:</strong> {{this.price}}</p>
                <p><strong>Código:</strong> {{this.code}}</p>
                <p><strong>Stock:</strong> {{this.stock}}</p>
                <p><strong>Categoría:</strong> {{this.category}}</p>
                <p><strong>Status:</strong> {{#if this.status}}Disponible{{else}}No disponible{{/if}}</p>
            </div>
        {{/each}}
    </div>
</div>

